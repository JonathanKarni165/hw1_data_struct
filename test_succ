from random import shuffle
from AVLTree import AVLTree
insert_seq = [4, 8, 1, 2, 9, 6, 3, 7, 5, 0]

print(insert_seq)

tree = AVLTree()
for i in range(len(insert_seq)):
    tree.insert(insert_seq[i], 'hi')
print('********')
tree.root.display()
print('******** test delete *****')

del_seq = [8, 6, 2, 5, 7, 9, 1, 0, 3, 4]

print(del_seq)
for i in range(10):
    print("deliting " + str(del_seq[i]))
    tree.delete(tree.search(del_seq[i]))
    print('\n-------------------\n\n')
    tree.root.display()
    #[4, 8, 1, 2, 9, 6, 3, 7, 5, 0]
    #[8, 6, 2, 5, 7, 9, 1, 0, 3, 4]